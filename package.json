{
  "name": "react-project-creator",
  "version": "1.0.0",
  "description": "A simple project generator with an ability to generate components and to set up basic libraries",
  "main": "index.js",
  "repository": "https://github.com/datarockets/react-project-creator.git",
  "author": "Vlad Oganov <v.oganov@datarockets.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js"
  },
  "bin": {
    "reactor": "yarn start"
  },
  "dependencies": {
    "inquirer": "^3.2.1",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "node-cmd": "^3.0.0",
    "node-yaml": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.1"
  }
}
